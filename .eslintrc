    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": 0,
        "no-global-assign": ["error", {"exceptions": ["console"]}]
